{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "appfactory20231031"
		},
		"adf to db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adf to db'"
		},
		"appdblinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'appdblinkedservice'"
		},
		"appvmserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'appvmserver'"
		},
		"csv to ddb_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'csv to ddb'"
		},
		"linkedService_Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService_Staging'"
		},
		"appvmserver_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.24.0\\logs"
		},
		"appvmserver_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "appuser"
		},
		"csv to ddb_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakedemodp203.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "appfactory20231031_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adf to db_connectionString": {
						"value": "[parameters('adf to db_connectionString')]"
					},
					"appdblinkedservice_connectionString": {
						"value": "[parameters('appdblinkedservice_connectionString')]"
					},
					"csv to ddb_accountKey": {
						"value": "[parameters('csv to ddb_accountKey')]"
					},
					"linkedService_Staging_connectionString": {
						"value": "[parameters('linkedService_Staging_connectionString')]"
					},
					"csv to ddb_properties_typeProperties_url": {
						"value": "[parameters('csv to ddb_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "appfactory20231031_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactory20231031_ArmTemplate_0')]"
			]
		},
		{
			"name": "appfactory20231031_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"appvmserver_password": {
						"value": "[parameters('appvmserver_password')]"
					},
					"appvmserver_properties_typeProperties_host": {
						"value": "[parameters('appvmserver_properties_typeProperties_host')]"
					},
					"appvmserver_properties_typeProperties_userId": {
						"value": "[parameters('appvmserver_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactory20231031_ArmTemplate_1')]"
			]
		},
		{
			"name": "appfactory20231031_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'appfactory20231031_ArmTemplate_2')]"
			]
		}
	]
}